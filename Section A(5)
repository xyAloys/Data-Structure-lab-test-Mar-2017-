#include <stdio.h>
int countOddDigits1(int num); 
void countOddDigits2(int num, int *result); 
int main()
{
	int number, result=-1;     
	printf("Enter the number: \n"); 
	scanf("%d", &number); 
	printf("countOddDigits1(): %d\n", countOddDigits1(number)); 
	countOddDigits2(number, &result); 
	printf("countOddDigits2(): %d\n", result); 
	scanf("\n", &number);
	return 0;
}
int countOddDigits1(int num) 
	{
	int temp;
	int count=0;
	while (num != 0)
	{
		temp = num;
		num= num / 10;
		if ((temp - (num*10)) % 2 !=0)
		{
			count++;
		} 
	
	}
	return count;
	} 
void countOddDigits2(int num, int *result) 
 {
	int temp;
	int count = 0;
	while (num != 0)
	{
		temp = num;
		num = num / 10;
		if ((temp - (num * 10)) % 2 != 0)
		{
			count++;
		}

	}
	*result = count;
	return;
}  
